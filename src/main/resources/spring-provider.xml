<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
       http://www.springframework.org/schema/beans/spring-beans.xsd
       http://code.alibabatech.com/schema/dubbo
       http://code.alibabatech.com/schema/dubbo/dubbo.xsd">

    <!--定义了提供方应用信息，用于计算依赖关系；在 dubbo-admin 或 dubbo-monitor 会显示这个名字，方便辨识-->
    <dubbo:application name="inschos-trading-provider" owner="programmer" organization="dubbox" />

    <!--使用 zookeeper 注册中心暴露服务，注意要先开启 zookeeper-->
    <dubbo:registry address="${registry.address}" group="${registry.group}"/>
    <!-- 用dubbo协议在20880端口暴露服务 -->
    <!--<dubbo:protocol name="dubbo" port="9900" />-->
    <!-- 用dubbo协议在20880端口暴露服务 -->
    <dubbo:protocol name="${registry.hessian.name}" port="${registry.hessian.port}" default="${registry.hessian.default}" serialization="json" />


    <!--<dubbo:service interface="com.inschos.cloud.trading.access.rpc.service.DemoService" ref="demoService" />-->
    <dubbo:annotation package="com.inschos.cloud.trading.access.rpc.service.impl" />






</beans>
