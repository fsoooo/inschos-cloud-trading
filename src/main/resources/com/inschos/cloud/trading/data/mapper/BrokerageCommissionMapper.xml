<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.inschos.cloud.trading.data.mapper.BrokerageCommissionMapper">

    <sql id="Base_Column_List">
        `id`,`private_code`,`product_rate`,`ditch_rate`,`agent_rate`,`product_money`,`ditch_money`,`agent_money`
    </sql>

    <insert id="addBrokerageCommission" parameterType="com.inschos.cloud.trading.model.BrokerageCommissionModel" useGeneratedKeys="true"
            keyProperty="id">
        insert into cust_warranty_brokerage
        (`id`,`private_code`,`product_rate`,`ditch_rate`,`agent_rate`,`product_money`,`ditch_money`,`agent_money`)
        values
        (#{id},#{private_code},#{product_rate},#{ditch_rate},#{agent_rate},#{product_money},#{ditch_money},#{agent_money})
    </insert>


</mapper>