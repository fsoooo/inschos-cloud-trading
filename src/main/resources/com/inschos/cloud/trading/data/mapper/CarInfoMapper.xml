<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.inschos.cloud.trading.data.mapper.CarInfoMapper">

    <sql id="Base_Column_List">
        `id`,`warranty_uuid`,`biz_id`,`thp_biz_id`,`insurance_type`,`car_code`,`name`,`code`,`phone`,`frame_no`,`engine_no`,`vehicle_fgw_code`,`vehicle_fgw_name`,`brand_code`,`is_not_car_code`,`is_trans`,`created_at`,`updated_at`
    </sql>

    <insert id="addCarInfo" parameterType="com.inschos.cloud.trading.model.CarInfoModel" useGeneratedKeys="true"
            keyProperty="id">
        insert into cust_warranty_car
        (`id`,`warranty_uuid`,`biz_id`,`thp_biz_id`,`insurance_type`,`car_code`,`name`,`code`,`phone`,`frame_no`,`engine_no`,`vehicle_fgw_code`,`vehicle_fgw_name`,`brand_code`,`is_not_car_code`,`is_trans`,`created_at`,`updated_at`)
        values
        (#{id},#{warranty_uuid},#{car_code},#{name},#{code},#{phone},#{frame_no},#{engine_no},#{vehicle_fgw_code},#{vehicle_fgw_name},#{brand_code},#{is_not_car_code},#{is_trans},#{created_at},#{updated_at})
    </insert>

    <select id="findOneByCarCode" parameterType="String" resultType="com.inschos.cloud.trading.model.CarInfoModel">
        select
        <include refid="Base_Column_List" />
        from cust_warranty_car where car_code=#{_parameter}
    </select>

    <select id="findByBizId" parameterType="String" resultType="com.inschos.cloud.trading.model.CarInfoModel">
        select
        `warranty_uuid`,`insurance_type`
        from cust_warranty_car where biz_id=#{_parameter}
    </select>

</mapper>
