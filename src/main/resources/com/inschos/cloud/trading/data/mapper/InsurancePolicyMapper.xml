    <select id="findEffectiveInsurancePolicyListByChannelId"
            parameterType="com.inschos.cloud.trading.model.InsurancePolicyModel"
            resultType="com.inschos.cloud.trading.model.InsurancePolicyModel">
        select
        <include refid="Base_Column_List"/>
        from cust_warranty WHERE `channel_id`=#{channel_id} AND `warranty_status`!= 1 AND `warranty_status`!= 6 AND
        state = 1
    </select>

    <select id="findEffectiveInsurancePolicyByChannelIdAndTime"
            parameterType="com.inschos.cloud.trading.model.InsurancePolicyModel"
            resultType="com.inschos.cloud.trading.model.InsurancePolicyModel">
        select
        <include refid="Base_Column_List"/>
        from cust_warranty
        <where>
            <choose>
                <when test="page.lastId>0">
                    id&gt;#{page.lastId}
                </when>
                <when test="page.start>0">
                    id&gt;=(select id from cust_warranty where manager_uuid=#{manager_uuid} AND state=1
                    <if test="start_time!=0">
                        and `start_time`&gt;=#{start_time}
                    </if>
                    <if test="end_time!=0">
                        and `start_time`$lt;#{end_time}
                    </if>
                    limit #{page.start},1
                    )
                </when>
                <otherwise>
                    1=1
                </otherwise>
            </choose>
            `channel_id`=#{channel_id} AND `warranty_status`!= 1 AND `warranty_status`!= 6 AND state = 1
            <if test="start_time!=0">
                and `start_time`&gt;=#{start_time}
            </if>
            <if test="end_time!=0">
                and `start_time`$lt;#{end_time}
            </if>
            limit #{page.offset}
        </where>
    </select>

